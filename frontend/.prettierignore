# Ignore artifacts
dist
node_modules
*.min.js
*.min.css

# Ignore configuration files
*.config.js
*.config.ts
vite.config.ts

# Ignore all dotfiles
.*
!.eslintrc.cjs
!.prettierrc.cjs
!.gitignore

# Ignore tests and snapshots
__tests__
